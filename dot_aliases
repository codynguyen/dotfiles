# Wealthica Management Instances
alias ssh-wprd="aws ssm start-session --target i-08bccc3e1cd7e6ba0 --profile prs"
alias ssh-wstg="aws ssm start-session --target i-0c478ffc9dddb848e --profile wstg"
alias ssh-vstg="aws ssm start-session --target i-0de30dc5b552ef00a --profile vstg"
alias ssh-vprd="aws ssm start-session --target i-050ebff70d21df11a --profile vprd"
alias login-prs="aws sso login --profile prs"

# Git
alias gs="git status"
alias gcm="git commit"
alias gcma="git commit -am"
alias gb="git branch"
alias gco="git checkout"
alias gp="git push"
alias gpl="git pull"
alias gcp="git cherry-pick"
alias gcpc="git cherry-pick --continue --abort"
alias gcpa="git cherry-pick"
alias ga="git add ."
alias gd="git diff --color"
alias grc="git rebase --continue"
alias gra="git rebase --abort"
alias grbm="git rebase origin/master"
alias grbp="git rebase origin/production"
alias grbi="git rebase --interactive"
alias gcmam="git commit --all --amend --no-edit"

# Docker
alias dc="docker compose"
alias dcps="docker compose ps"
alias dcst="docker compose start"
alias dcstp="docker compose stop"
alias dcu="docker compose up -d"
alias dcr="docker compose run --rm"
alias dcl="docker compose logs --tail 100 -f"
